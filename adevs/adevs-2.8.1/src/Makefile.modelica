INITIALIZATION_OBJS = initialization$(OBJ_EXT) initialization_data$(OBJ_EXT) method_ipopt$(OBJ_EXT) method_kinsol$(OBJ_EXT) method_newuoa$(OBJ_EXT) method_simplex$(OBJ_EXT) method_nelderMeadEx$(OBJ_EXT)
