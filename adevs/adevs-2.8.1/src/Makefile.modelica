gc/roots$(OBJ_EXT) \
gc/generational$(OBJ_EXT)  \
gc/marksweep$(OBJ_EXT)

META_HFILES = \
rml_compatibility.h \
meta_modelica_builtin.h \
meta_modelica_builtin_boxptr.h \
meta_modelica_real.h \
meta_modelica_string_lit.h \
meta_modelica_segv.h \
meta_modelica.h \
gc/mmc_gc.h \
gc/common.h \
gc/generational.h \
gc/marksweep.h \
